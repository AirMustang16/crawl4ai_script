# Run browser with visible window for debugging
HEADFUL=1

# Timeouts (milliseconds)
PAGE_TIMEOUT_MS=120000
WAIT_FOR_TIMEOUT_MS=400000
KEEP_BROWSER_OPEN=1
# Number of times to click "Load more/Next"
PAGES_TO_SCRAPE=3
# Optional LLM fallback (disabled by default)
ENABLE_LLM_FALLBACK=0
# LLM_PROVIDER can be e.g. "ollama/llama3.3" or "openai/gpt-4o"
LLM_PROVIDER=ollama/llama3.3
# Set only if using a remote provider like OpenAI
# LLM_API_TOKEN=your_api_token_here
